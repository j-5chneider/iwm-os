@techreport{anvari_replicability_2019,
	type = {{WorkingPaper}},
	title = {The {Replicability} {Crisis} and {Public} {Trust} in {Psychological} {Science}: {REPLICABILITY} {CRISIS} {AND} {PUBLIC} {TRUST}},
	abstract = {Although science is generally regarded highly by the public (Jonge, 2015), replication failures of past findings in several scientific disciplines, including psychology, medicine, and experimental economics, have created a ‘crisis of confidence’ among scientists. Psychological science has been at the forefront of these discussions, with discussions about replication failures and scientific self-criticisms of questionable research practices (QRPs) increasingly taking place on social media and gaining considerable news coverage. Whether this replicability crisis and its surrounding debates impacts the public’s trust positively or negatively is a question yet to be answered by research (Rutjens, Heine, \& Sutton, 2017). Whereas some researchers believe that the public’s trust will be positively impacted or maintained (e.g., Vazire, 2016) others believe replication failures and criticisms of QRPs will diminish trust (e.g., Fanelli, 2018). Because it is our field of expertise, we will focus on trust in the discipline of psychology for the current paper. We propose a study that tests how public trust in past and future psychological research, and the psychological science community more generally, will be impacted by being informed about either i) replication failures, ii) replication failures and criticisms of QRPs, iii) replication failures, criticisms of QRPs, and proposed reforms, or iv) when they are not informed about any of these.},
	language = {English},
	institution = {Comprehensive Results in Social Psychology},
	author = {Anvari, Farid and Lakens, Daniel},
	year = {2019},
	keywords = {Open Science, replicability, reproducibility, scientific trust, trust in science, trustworthiness, Open Science Practices},
	file = {Replicability crisis and public trust - CRSP no prescreen-1.docx:C\:\\Users\\Juergen Schneider\\Zotero\\storage\\83WBRVPG\\Replicability crisis and public trust - CRSP no prescreen-1.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document}
